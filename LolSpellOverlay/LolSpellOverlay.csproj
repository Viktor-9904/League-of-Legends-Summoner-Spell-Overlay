    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Icons\App\AppIcon2.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>

    <None Remove="Icons\SummonerSpells\Heal.png" />
    <None Remove="Icons\SummonerSpells\Ignite.png" />
    <None Remove="Icons\SummonerSpells\Smite.png" />
    <None Remove="Icons\SummonerSpells\Teleport.png" />
    <None Remove="Icons\SummonerSpells\Unleashed_Teleport.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Icons\App\AppIcon2.ico" />

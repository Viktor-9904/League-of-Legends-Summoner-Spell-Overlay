    <None Remove="Icons\Lanes\Support.png" />
    <None Remove="Icons\Lanes\Top.png" />
    <None Remove="Icons\SummonerSpells\Barrier.png" />
    <None Remove="Icons\SummonerSpells\Cleanse.png" />
    <None Remove="Icons\SummonerSpells\Exhaust.png" />
    <None Remove="Icons\SummonerSpells\Flash.png" />

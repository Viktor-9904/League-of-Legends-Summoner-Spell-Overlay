    <EmbeddedResource Remove="SummonerSpellIcons\**" />
    <None Remove="LaneIcons\**" />
    <None Remove="SummonerSpellIcons\**" />
    <Page Remove="LaneIcons\**" />
    <Page Remove="SummonerSpellIcons\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Icons\App\AppIcon.ico" />
